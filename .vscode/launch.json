{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run veronica",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
            "preLaunchTask": "${defaultBuildTask}"
        }
    ]
}
